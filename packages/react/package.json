{
  "name": "@crit-fumble/react",
  "version": "19.10.4",
  "description": "Shared React component library for Crit-Fumble projects",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./shared": {
      "types": "./dist/shared/index.d.ts",
      "import": "./dist/shared/index.js"
    },
    "./desktop": {
      "types": "./dist/desktop/index.d.ts",
      "import": "./dist/desktop/index.js"
    },
    "./web": {
      "types": "./dist/web/index.d.ts",
      "import": "./dist/web/index.js"
    },
    "./activity": {
      "types": "./dist/activity/index.d.ts",
      "import": "./dist/activity/index.js"
    },
    "./styles": {
      "import": "./dist/styles.css"
    },
    "./tailwind": {
      "import": "./tailwind.config.js"
    }
  },
  "files": [
    "dist",
    "tailwind.config.js"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:lib": "tsc -b && vite build --mode lib",
    "preview": "vite preview",
    "lint": "eslint .",
    "test": "jest --config jest.config.cjs",
    "test:watch": "jest --config jest.config.cjs --watch",
    "test:coverage": "jest --config jest.config.cjs --coverage",
    "test:visual": "playwright test",
    "test:visual:update": "playwright test --update-snapshots",
    "test:visual:ui": "playwright test --ui",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepublishOnly": "npm run build:lib",
    "release": "npm run build:lib && npm publish --access public",
    "release:patch": "npm version patch && npm run release",
    "release:minor": "npm version minor && npm run release",
    "release:major": "npm version major && npm run release"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "clsx": "^2.1.1",
    "framer-motion": "^11.18.2",
    "react-rnd": "^10.5.2",
    "zustand": "^4.5.7"
  },
  "dependencies": {
    "rpg-awesome": "^0.2.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.6.1",
    "@crit-fumble/core": "^10.7.0",
    "@playwright/test": "^1.57.0",
    "clsx": "^2.1.1",
    "framer-motion": "^11.18.2",
    "react-rnd": "^10.5.2",
    "zustand": "^4.5.7",
    "@storybook/addon-essentials": "^8.1.11",
    "@storybook/addon-interactions": "^8.1.11",
    "@storybook/addon-links": "^8.1.11",
    "@storybook/blocks": "^8.1.11",
    "@storybook/react": "^8.1.11",
    "@storybook/react-vite": "^8.1.11",
    "@storybook/test": "^8.1.11",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/react-rnd": "^7.4.4",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^4.0.14",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "eslint-plugin-storybook": "^0.8.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jsdom": "^24.1.0",
    "postcss": "^8.4.39",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "storybook": "^8.1.11",
    "tailwindcss": "^3.4.4",
    "ts-jest": "^29.4.6",
    "typescript": "^5.5.3",
    "vite": "^7.2.6",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.14"
  },
  "keywords": [
    "react",
    "components",
    "ui",
    "storybook",
    "tailwindcss",
    "discord",
    "crit-fumble"
  ],
  "author": "Crit-Fumble Gaming",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Crit-Fumble/storybook.git",
    "directory": "packages/react"
  },
  "homepage": "https://storybook.crit-fumble.com"
}
